<?php
class Model_Catfood_Allbooklist{
    public function getBooklist($option){
        $allbook = new Dr_Booklist();
        $allbooklist = $allbook -> getBooklist($option);
        return $allbooklist;
    }
    public function getMybooklist($option){
        $mybook = new Dr_Mybooklist();
        $mybooklist = $mybook->getMybooklist($option);
        return $mybooklist;
    }
    public function getHistorybooklist($option){
        $hisbook = new Dr_Historybooklist();
        $hisbooklist = $hisbook->getHistorybooklist($option);
        return $hisbooklist;
    }
}