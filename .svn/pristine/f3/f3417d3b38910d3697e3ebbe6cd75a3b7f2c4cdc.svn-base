[ent]
;关闭自动加载，启用手动加载类库
application.library=APP_PATH "/empty"
application.system.library=PLATFORM_PATH "/empty"
application.system.use_spl_autoload=true

application.bootstrap=PLATFORM_PATH "/bootstrap.php"
application.directory=APP_PATH
;application.dispatcher.throwException=true
application.dispatcher.catchException=true

application.dispatcher.defaultRoute.type="map"
application.dispatcher.defaultRoute.controllerPrefer=true

[product:ent]
;application.subs="sub1 sub2"

